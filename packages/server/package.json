{
  "name": "@example/server",
  "version": "1.0.0",
  "scripts": {
    "build": "node ../../build.mjs && tsc -p ./tsconfig.build.json",
    "test": "concurrently \"npm:test:*\"",
    "test:bun": "bun wiptest tests/bun",
    "test:node": "npx tsx ./tests/node",
    "test:worker": "node ../../worker-tests.mjs ./tests/worker/*.test.ts"
  },
  "source": "src/server.ts",
  "exports": {
    "types": "./dist/server.d.ts",
    "require": "./dist/server.cjs",
    "default": "./dist/server.mjs"
  },
  "imports": {
    "#platform": {
      "bun": "@example/bun",
      "node": "@example/node",
      "worker": "@example/worker"
    }
  },
  "optionalPeerDependencies": {
    "@example/bun": "1.0.0",
    "@example/node": "1.0.0",
    "@example/worker": "1.0.0"
  },
  "dependencies": {
    "@example/platform": "1.0.0",
    "cookie": "^0.5.0"
  },
  "devDependencies": {
    "@example/bun": "1.0.0",
    "@example/node": "1.0.0",
    "@example/worker": "1.0.0",
    "@types/cookie": "^0.5.1",
    "@types/node": "18.8.3",
    "bun-types": "^0.1.11",
    "concurrently": "7.4.0",
    "npx": "10.2.2",
    "typescript": "^4.8.4"
  },
  "sideEffects": false
}
